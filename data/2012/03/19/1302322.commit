-                        &lt;/xs:element&gt;
-                    &lt;/xs:sequence&gt;
-                &lt;/xs:complexType&gt;
-            &lt;/xs:element&gt;
-        &lt;/xs:schema&gt;
-    &lt;/localEntry&gt;
-
-    &lt;in&gt;
-        &lt;validate&gt;
