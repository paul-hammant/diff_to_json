-                &lt;log level=&quot;full&quot; /&gt;
-            &lt;/outSequence&gt;
-        &lt;/target&gt;
-    &lt;parameter name=&quot;transport.fix.AcceptorConfigURL&quot;&gt;
-        file:repository/conf/sample/resources/fix/fix-synapse.cfg
-    &lt;/parameter&gt;
