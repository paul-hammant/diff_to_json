-                        &lt;/endpoint&gt;
-                    &lt;/send&gt;
-                &lt;/case&gt;
-                &lt;case regex=&quot;MSFT&quot;&gt;
-                    &lt;send&gt;
-                        &lt;endpoint&gt;
-                            &lt;address
-                                uri=&quot;fix://localhost:19877?BeginString=FIX.4.1&amp;amp;SenderCompID=SYNAPSE&amp;amp;TargetCompID=EXEC&quot; /&gt;
-                        &lt;/endpoint&gt;
-                    &lt;/send&gt;
